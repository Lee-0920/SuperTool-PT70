QPushButton#transparentButton{
        background:transparent;
}

QPushButton#brownButton{
        border-radius:5px;
        border:1px solid gray;
        background:rgb(85, 170, 200);
        color:white;
        border-style: outset;
}

QPushButton#brownButton::pressed{
        color:rgb(173, 115, 0);
        background:white;
}

QPushButton#brownButton:!enabled{
        color:black;
        background:rgb(204, 204, 204);
}

