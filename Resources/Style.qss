QTabWidget::pane{
    border:none;
}

QTabBar::tab{
    background:rgb(100, 150, 150);
    color:white;
    width:15ex;
    height:6ex;
    font: bold 13px;
}
QTabBar::tab:hover{
        background:rgb(115, 172, 172, 100);
}
QTabBar::tab:selected{
    border-color: white;
    background:white;
    color:rgb(145, 70, 0);
    font: bold 13px;
}

